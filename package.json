{
  "name": "runtime-typescript-checker-demo",
  "version": "1.0.0",
  "description": "Demo of runtime-typescript-checker",
  "main": "index.ts",
  "dependencies": {
    "ajv": "^8.12.0",
    "typescript-json-schema": "^0.55.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "runtime-typescript-checker": "^1.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build-example1": "cd src && node.exe -r ts-node/register ./example1/index.ts --build",
    "build-example1-force": "cd src && node.exe -r ts-node/register ./example1/index.ts --build --force",
    "test-example1": "cd src && node.exe -r ts-node/register ./example1/index.ts --test"
  },
  "keywords": [
    "typescript",
    "ts",
    "runtime",
    "automatic",
    "types",
    "interfaces",
    "code generator",
    "javascript"
  ],
  "author": "Tomas Nilsson",
  "license": "ISC",
  "homepage": "https://github.com/tomnil/runtime-typescript-checker-demo"
}
